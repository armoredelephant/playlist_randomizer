    ],
    "moduleNameMapper": {
      "@A(.*)$": "<rootDir>/src/components/_atoms/$1",
      "@M(.*)$": "<rootDir>/src/components/_molecules/$1",
      "@O(.*)$": "<rootDir>/src/components/_organisms/$1"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --list-different"
    ]
  }
